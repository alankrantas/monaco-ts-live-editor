{
    "name": "monaco-ts-live-editor",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "upgrade-all": "yarn-upgrade-all && yarn upgrade",
        "dev": "vite --port 3000 --open --host",
        "start": "yarn dev",
        "build": "vite build",
        "preview": "vite preview --port 8080 --open --host",
        "serve": "yarn preview",
        "pull": "git pull origin main",
        "push": "git add . && git commit -m \"User updating\" && git push origin main",
        "commit": "yarn pull && yarn push"
    },
    "dependencies": {
        "@monaco-editor/react": "^4.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "typescript": "^5.8.2",
        "vite": "^6.0.11",
        "yarn-upgrade-all": "^0.7.5"
    }
}
